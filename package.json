{
  "name": "vfile-visit",
  "version": "1.1.0",
  "description": "visit and convert vfile contents to vfiles",
  "main": "index.js",
  "scripts": {
    "xo": "xo --fix --space --no-esnext index.js",
    "tape": "tape test.js",
    "report": "nyc report --reporter=text-lcov | coveralls",
    "pretest": "npm run xo",
    "test": "nyc npm run tape"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mrzmmr/vfile-visit.git"
  },
  "keywords": [
    "virtual",
    "file",
    "vfile",
    "visit",
    "walk"
  ],
  "author": "Paul Zimmer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mrzmmr/vfile-visit/issues"
  },
  "homepage": "https://github.com/mrzmmr/vfile-visit#readme",
  "dependencies": {
    "clone": "^2.1.1",
    "vfile": "^2.3.0"
  },
  "devDependencies": {
    "coveralls": "^3.0.2",
    "tape": "^4.8.0",
    "xo": "^0.23.0",
    "nyc": "^13.1.0"
  }
}
